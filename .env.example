# Railway Environment Variables for DearLock Backend

# Database Configuration (Railway will provide these)
DB_HOST=your_railway_mysql_host
DB_NAME=railway
DB_USER=root
DB_PASS=your_railway_mysql_password

# Application Configuration
APP_ENV=production
APP_DEBUG=false

# CORS Configuration
CORS_ORIGIN=https://dearlock.netlify.app

# Session Configuration
SESSION_SECURE=true